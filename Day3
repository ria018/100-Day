package penjumlahan;
public class Penjumlahan {
    public static void main(String[] args) {
        int angka = 18;
        int angka2 = 10;
        int angka3 = 4;
        System.out.println(angka + angka2 + angka3);
        System.out.println(angka + angka2);
        System.out.println(angka + angka3);
    }
    
}
